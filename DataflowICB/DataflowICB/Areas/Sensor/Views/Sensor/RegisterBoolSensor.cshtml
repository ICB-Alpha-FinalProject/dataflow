@model SensorViewModel
@using (Ajax.BeginForm("CreateSensor", "Sensor",
        new AjaxOptions
        {
            HttpMethod = "POST",
            UpdateTargetId = "form-container",
            InsertionMode = InsertionMode.Replace,
            OnComplete = "onComplete"

        }))
{
    @Html.AntiForgeryToken()

    @Html.Partial("_CommonSensorForm", Model)

    <input type="submit" value="Register" class="" />
}

